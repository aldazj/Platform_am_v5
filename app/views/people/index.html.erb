<div id="page-wrapper">
  <div class="row">
    <div class="col-lg-12">
      <h1>Users  <small>Listing Users</small></h1>
      <ol class="breadcrumb">
        <li class="active"><i class="fa fa-dashboard"></i> Listing Users </li>
      </ol>
    </div>
  </div>

  <%= form_tag people_path, :method => 'get', :id => 'people_search' do %>
      <p>
        <%= text_field_tag :q, params[:q] %>
        <%= submit_tag "Search", :lastname => nil %>
      </p>
      <div>
        <div class="row">
          <div class="col-lg-6">
            <div id="people_div"> <%= render 'people' %> </div>
          </div>
        </div><!-- /.row -->
      </div>
  <% end %>

  <%= link_to 'New Person', new_person_path, class:'btn btn-default' %>
  <br>
</div>

